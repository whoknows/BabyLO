<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BabyPlayed" table="baby_played">
    <indexes>
      <index name="id_player" columns="id_player"/>
      <index name="id_game" columns="id_game"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="team" type="integer" column="team" nullable="false"/>
    <many-to-one field="idGame" target-entity="BabyGame">
      <join-columns>
        <join-column name="id_game" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPlayer" target-entity="BabyPlayer">
      <join-columns>
        <join-column name="id_player" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
